<p id="notice"><%= notice %></p>

<h1>Pub Quiz Answers</h1>

<table>
  <thead>
    <tr>
      <th>Teamname</th>
      <th>Created at</th>
      <th>Updated at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pub_quiz_answers.each do |pub_quiz_answer| %>
      <tr>
        <td><%= pub_quiz_answer.teamname %></td>
        <td><%= pub_quiz_answer.created_at %></td>
        <td><%= pub_quiz_answer.updated_at %></td>
        <td><%= link_to 'Show', pub_quiz_answer_path(pub_quiz_answer) %></td>
        <td><%= link_to 'Edit', edit_pub_quiz_answer_path(pub_quiz_answer) %></td>
        <td><%= link_to 'Destroy', pub_quiz_answer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pub Quiz Answer', new_pub_quiz_answer_path %>
